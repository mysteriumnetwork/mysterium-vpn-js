    "@babel/runtime": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
