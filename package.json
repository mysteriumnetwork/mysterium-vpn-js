{
  "name": "mysterium-tequilapi",
  "version": "0.4.2",
  "description": "Api library to control mysterium client and node",
  "repository": "github:mysteriumnetwork/mysterium-tequilapi",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "./node_modules/.bin/karma start ./test/karma.conf.js",
    "ci": "npm run flow && npm run test && npm run lint",
    "lint": "eslint --format 'node_modules/eslint-formatter-friendly' ./src ./test",
    "build": "npx tsc -p src && ./scripts/generate_flow_definitions.sh",
    "prepack": "npm run prepack:babel && npm run prepack:flow && npm run prepack:ts",
    "prepack:babel": "babel src/ -d lib",
    "prepack:flow": "flow-copy-source src lib",
    "prepack:ts": "cp types/index.d.ts lib/",
    "flow-typed": "npm i flow-typed && node_modules/.bin/flow-typed install axios",
    "flow": "node_modules/.bin/flow"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@types/axios": "^0.14.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-friendly": "^6.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.80.0",
    "flow-copy-source": "^2.0.2",
    "flow-typed": "^2.5.1",
    "flowgen": "^1.5.8",
    "karma": "^3.1.1",
    "karma-browserify": "^5.3.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.32",
    "mocha": "^5.2.0",
    "typescript": "^3.3.3",
    "watchify": "^3.11.0"
  },
  "keywords": [
    "mysterium",
    "network",
    "tequilapi",
    "vpn"
  ],
  "author": {
    "name": "Andrej Novikov",
    "email": "jazzaiman@gmail.com"
  },
  "license": "MIT"
}
